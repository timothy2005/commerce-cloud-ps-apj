$catalogVersion = catalogVersion(catalog(id[default=$pcmCatalog]), version[default=$version])[unique=true, default=$pcmCatalog:$version]
$languages = en, en_AU, en_NZ

# Product Bunnings Catalog
INSERT_UPDATE Catalog; id[unique = true]; name
                     ; $pcmCatalog      ; Bunnings PCM

INSERT_UPDATE Language; isoCode[unique = true]; name        ; active
                      ; en_AU                 ; AUSTRALIAN  ; TRUE
