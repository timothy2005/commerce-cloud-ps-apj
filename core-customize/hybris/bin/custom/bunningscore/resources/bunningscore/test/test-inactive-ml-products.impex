$pcmCatalog = BunningsPCMCatalog
$version = Staged

$catalogVersion = catalogVersion(catalog(id[default=$pcmCatalog]), version[default=$version])[unique=true, default=$pcmCatalog:$version]
$supercategories = supercategories(code, $catalogVersion)[unique=true,PRODUCTINDEX_1 mode=append]
$variantType = variantType(code)[default='GenericVariantProduct']
$approvalStatus = approvalstatus(code)[default='approved'][unique=true]
$baseProduct = baseProduct(code, $catalogVersion)[unique=true]

