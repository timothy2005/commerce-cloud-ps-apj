{
    "name": "smartedit-e2e",
    "version": "0.0.1",
    "private": true,
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "description": "SmartEdit E2E",
    "scripts": {
        "clean": "rimraf ./generated",
        "build": "",
        "e2e": "npm-run-all prepare webpack -p -r connect protractor",
        "prepare": "npm run clean && npm run instrument",
        "instrument": "node ./scripts/instrument-se.js",
        "webpack": "npm run webpack:base && npm run webpack:apps && npm run webpack:tests",
        "webpack:base": "npm run webpack:container-base && npm run webpack:inner-base",
        "webpack:apps": "npm run webpack:container-apps && npm run webpack:inner-apps",
        "webpack:container-apps": "webpack --config=webpack/webpack.container-apps.js",
        "webpack:inner-apps": "webpack --config=webpack/webpack.inner-apps.js",
        "webpack:container-base": "webpack --config=webpack/webpack.container-base.js",
        "webpack:inner-base": "webpack --config=webpack/webpack.inner-base.js",
        "webpack:tests": "webpack --config=webpack/webpack.tests.js",
        "e2e:debug": "npm-run-all prepare webpack -p -r connect protractor:debug",
        "e2e:max": "npm-run-all prepare webpack -p -r connect protractor:max",
        "dev": "npm-run-all prepare webpack connect",
        "connect": "npm-run-all -p connect:test connect:dummystorefront",
        "connect:test": "node ./scripts/connect-server.js --target=testapp",
        "connect:dummystorefront": "node ./scripts/connect-server.js --target=dummystorefront",
        "protractor": "node ./scripts/run-protractor.js --headless",
        "protractor:debug": "node ./scripts/run-protractor.js",
        "protractor:max": "node ./scripts/run-protractor.js --headless --multi",
        "e2e:ci": "npm-run-all prepare webpack -p -r connect \"protractor -- {@}\" --",
        "lint": "npm run lint:base -- 'tests/**/*.{ts,js}'",
        "lint:base": "eslint"
    },
    "devDependencies": {
        "@types/angular": "1.7.3",
