{
  "version": "0.0.1",
  "private": true,
  "name": "smarteditmaster",
  "description": "Master SmartEdit application",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "npm run build-container:dev && npm run build-inner:dev",
    "build:prod": "npm run build-container:prod && npm run build-inner:prod",
    "build-inner:dev": "npm run build-inner-only:dev && npm run copy-to-webroot -- --inner",
    "build-inner:prod": "npm run build-inner-only:prod && npm run copy-to-webroot -- --inner",
    "build-container:dev": "npm run build-container-only:dev && npm run copy-to-webroot -- --container",
    "build-container:prod": "npm run build-container-only:prod && npm run copy-to-webroot -- --container",
    "build-inner-only:dev": "webpack --config=webpack/inner/webpack.dev.js",
    "build-inner-only:prod": "webpack --config=webpack/inner/webpack.prod.js",
    "build-container-only:dev": "webpack --config=webpack/container/webpack.dev.js",
    "build-container-only:prod": "webpack --config=webpack/container/webpack.prod.js",
    "copy-to-webroot": "node ./scripts/copy-to-webroot.js",
    "lint": "npm run lint:base -- 'src/**/*.ts'",
    "lint:base": "eslint"
  },
